#!/usr/bin/env sh -l

source $(dirname $(dirname $0))/lib/utils.sh

expected_asdf_command_path="$(asdf_dir)/commands/asdf-$1.sh"

# check if command exists
if [ -f $expected_asdf_command_path ]; then
  asdf_command_path=$expected_asdf_command_path
else
  # run help command
  asdf_command_path="$(asdf_dir)/commands/asdf-help.sh"
fi


callback_args="${@:2}"
$SHELL $asdf_command_path $callback_args
